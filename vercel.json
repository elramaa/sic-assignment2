{
    "builds": [
        {
            "src": "flask/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "flask/app.py"
        }
    ]
}